package defaul;

public class defaul {public static void main(String[] args) {
		int x = -100;
		int result1 = +x; // 부호 +를 붙임. result 1?
		int result2 = -x; // 부호 -를 붙임 
		System.out.println("(+x) result1 = " + result1);
		System.out.println("(+x) x *-1" + x * -1);
		System.out.println("(-x) result2 = " + result2 );
		System.out.println("(-x) x *-1" + x * -1);
		
		//실생활에서 앞에 + 기호를 붙인것을 본 적 없음
		// 없음. - <- 부호는  부호 반전을 시키는 규칙이 있음( +는 아무 규칙 업승ㅁ


}

}
