package pack01_if;

public class ex_01_if {public static void main(String[] args) {//{} <= 프로그램의 시작점
	// if문 : 제어문 중에 조건문이다.
	// 어떤 조건을 주고 해당 조건이 TRUE일 대의 지역(블럭킹{})을 만들고 그 지역의 코드를 실행한다.
	// 이항연산자부분은 값만 결과로 사용 가능. => if 문은 별도의 코딩을 결과로써 사용이 가능하다
	//if (조건식※ T와 F의 결과만 얻을 수 있는 것※ {
	// 조건식이 TRUE 일 때 시작하는 부분.
	//} 
	
	int score = 93;
	if(score) > = 90{
		System.out.println('점수가 90보다 큽니다.');
		
	}
	
	
	
	
	
	
	
}// 프로그램의 끝점

}
