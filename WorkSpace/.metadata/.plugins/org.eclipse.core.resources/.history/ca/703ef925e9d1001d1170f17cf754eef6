package ex05oper;

public class ex05oper {
	public static void main(String[] args) {
		int result = 0;
		// result에 10을 더한 결과 다시 result 할당
		// result에 10을 더한 결과 다시 result 할당
		// 결과값 20
		System.out.println(result + 10);
		result = result+10; // result = 0+ 10 대입연산자
		result = result+10;
		System.out.println(result);
		result += 10;
		result += 10;
		System.out.println(result + 10);
	}

}
